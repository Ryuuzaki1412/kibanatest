{
  "type": "plugin",
  "id": "@kbn/discover-plugin",
  "owner": [
    "@elastic/kibana-data-discovery"
  ],
  "description": "This plugin contains the Discover application and the saved search embeddable.",
  "plugin": {
    "id": "discover",
    "browser": true,
    "server": true,
    "requiredPlugins": [
      "charts",
      "data",
      "dataViews",
      "embeddable",
      "inspector",
      "fieldFormats",
      "urlForwarding",
      "navigation",
      "uiActions",
      "savedObjects",
      "savedObjectsFinder",
      "savedObjectsManagement",
      "savedSearch",
      "dataViewFieldEditor",
      "dataViewEditor",
      "expressions",
      "unifiedDocViewer",
      "unifiedSearch",
      "unifiedHistogram",
      "contentManagement"
    ],
    "optionalPlugins": [
      "home",
      "share",
      "usageCollection",
      "spaces",
      "triggersActionsUi",
      "savedObjectsTaggingOss",
      "lens",
      "noDataPage",
      "globalSearch"
    ],
    "requiredBundles": [
      "kibanaUtils",
      "kibanaReact",
      "unifiedSearch"
    ],
    "extraPublicDirs": [
      "common"
    ],
    "__category__": {
      "oss": true,
      "example": false,
      "testPlugin": false
    }
  }
}